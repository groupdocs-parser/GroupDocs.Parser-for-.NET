<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <Import Project="..\GroupDocs.Parser.Examples.CSharp\GroupDocs.Parser.Examples.CSharp.projitems" Label="Shared" />

  <ItemGroup>
    <PackageReference Include="GroupDocs.Parser" Version="25.8.0" />
	<PackageReference Include="Aspose.OCR" Version="22.11.0" />
	<PackageReference Include="System.Data.SQLite.Core" Version="1.0.112" />
	<PackageReference Include="Aspose.Drawing.Common" Version="24.12.0">
      <Aliases>AsposeDrawingCommon</Aliases>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Aspose.Drawing">
      <HintPath>$(NuGetPackageRoot)aspose.ocr\22.11.0\lib\netstandard2.0\Aspose.Drawing.dll</HintPath>
      <Aliases>OCR</Aliases>
    </Reference>
  </ItemGroup>

</Project>
